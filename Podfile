target 'FlickrClient' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FlickrClient
  pod 'SwiftyJSON'
  
  target 'FlickrClientTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FlickrClientUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

